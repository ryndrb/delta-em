script MeteorFalls_1F_1R_EventScript_DoubleBattle {
	if(var(VAR_TEMP_1) == 2){
		switch(var(VAR_TEMP_0)){
			case 1: applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceRight)
			case 2: applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceDown)
		}
		waitmovement(LOCALID_RIVAL)
		checkplayergender
		if(var(VAR_RESULT) == MALE){
			msgbox("{COLOR RED}May{COLOR DARK_GRAY}: There are two of them…\nAnd two of us…\p{PLAYER}, please!\nBattle together with me!", MSGBOX_YESNO)
			if(var(VAR_RESULT) == NO){
				msgbox("What!?\pAh!\nI get it!\pYou've got to get ready first?\nI'll hold them here, so hurry!!")
				closemessage
				applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
				applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceLeft)
				waitmovement(OBJ_EVENT_ID_PLAYER)
				release
				end
			}
			msgbox("Thanks a mill!")
		}else{
			msgbox("{COLOR RED}Brendan{COLOR DARK_GRAY}: There's two of them…\nAnd two of us, huh?\pSorry to ask, {PLAYER}, but…\nBattle alongside me!", MSGBOX_YESNO)
			if(var(VAR_RESULT) == NO){
				msgbox("So even you get nervous, huh…\pI get it.\nI'll wait till you get ready.")
				closemessage
				applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
				applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceLeft)
				waitmovement(OBJ_EVENT_ID_PLAYER)
				release
				end
			}
			msgbox("Thanks, pal!")
		}
		closemessage
	}
	applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceLeft)
	switch(var(VAR_TEMP_0)){
		case 1: 
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkDown)
			waitmovement(OBJ_EVENT_ID_PLAYER)
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkLeft)
		case 2: 
			applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkLeft)
	}
	waitmovement(OBJ_EVENT_ID_PLAYER)
	delay(8)
	playse(SE_PIN)
	applymovement(LOCALID_TABITHA, Common_Movement_QuestionMark)
	waitse
	waitmovement(LOCALID_TABITHA)
	msgbox("{COLOR RED}???{COLOR DARK_GRAY}: Ahyahyahya!\nIsn't this amusing!?\pIs it really your intention to oppose\nme, a Team Magma Admin, just the two\lof you?\pMmm…\nVery well then!\pI'll show you how terrifying adults can\nbe! Ahyahyahya!\pI'm going to give you some very\nthorough coaching!")
	delay(8)
	checkplayergender
	if(var(VAR_RESULT) == MALE){
		msgbox("{COLOR RED}May{COLOR DARK_GRAY}: This'll be my first time battling\nbeside you, {PLAYER}!\pI've got to do my best!")
	}else{
		msgbox("{COLOR RED}Brendan{COLOR DARK_GRAY}: All right!\nLet's do this!")
	}
	closemessage
	delay(8)
	call(MeteorFalls_1F_1R_EventScript_ChoosePartyForMultiBattle)
	return
}

script MeteorFalls_1F_1R_EventScript_ChoosePartyForMultiBattle {
	special(HealPlayerParty)
	choose_mons
	if(var(VAR_RESULT) == 0){
		goto(MeteorFalls_1F_1R_EventScript_CancelledMultiBattle)
	}
	checkplayergender
	if(var(VAR_RESULT) == MALE){
		switch(var(VAR_STARTER_MON)){
			case 0: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_MAY_METEOR_FALLS_TORCHIC, TRAINER_BACK_PIC_MAY)
			case 1: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_MAY_METEOR_FALLS_MUDKIP, TRAINER_BACK_PIC_MAY)
			case 2: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_MAY_METEOR_FALLS_TREECKO, TRAINER_BACK_PIC_MAY)
		}
	}else{
		switch(var(VAR_STARTER_MON)){
			case 0: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_BRENDAN_METEOR_FALLS_TORCHIC, TRAINER_BACK_PIC_BRENDAN)
			case 1: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_BRENDAN_METEOR_FALLS_MUDKIP, TRAINER_BACK_PIC_BRENDAN)
			case 2: multi_2_vs_2(TRAINER_GRUNT_MAGMA_METEOR_FALLS, MeteorFalls_1F_1R_Text_MagmaGruntDefeat, TRAINER_TABITHA_METEOR_FALLS, MeteorFalls_1F_1R_Text_TabithaDefeat, TRAINER_BRENDAN_METEOR_FALLS_TREECKO, TRAINER_BACK_PIC_BRENDAN)
		}
	}
	specialvar(VAR_RESULT, GetBattleOutcome)
	if(var(VAR_RESULT) == B_OUTCOME_LOST){
		fadescreen(FADE_TO_BLACK)
		special(Script_FadeOutMapMusic)
		waitstate
		special(SetCB2WhiteOut)
		waitstate
		end
	}
	special(HealPlayerParty)
	return
}

script MeteorFalls_1F_1R_EventScript_CancelledMultiBattle {
	applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceDown)
	waitmovement(LOCALID_RIVAL)
	checkplayergender
	if(var(VAR_RESULT) == MALE){
		msgbox("What!?\pAh!\nI get it!\pYou've got to get ready first?\nI'll hold them here, so hurry!!")
		closemessage
		applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
		applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceLeft)
		waitmovement(OBJ_EVENT_ID_PLAYER)
		applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
		waitmovement(OBJ_EVENT_ID_PLAYER)
	}else{
		msgbox("So even you get nervous, huh…\pI get it.\nI'll wait till you get ready.")
		closemessage
		applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
		applymovement(LOCALID_RIVAL, Common_Movement_WalkInPlaceLeft)
		waitmovement(OBJ_EVENT_ID_PLAYER)
		applymovement(OBJ_EVENT_ID_PLAYER, Common_Movement_WalkRight)
		waitmovement(OBJ_EVENT_ID_PLAYER)
	}
	release
	end
}

