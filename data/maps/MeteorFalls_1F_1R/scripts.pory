	walk_right * 6
	walk_up * 4
	walk_in_place_right
}

movement MeteorFalls_1F_1R_Movement_ArchieGotoScene {
	walk_right * 7
	walk_up * 3
	walk_in_place_right
}

movement MeteorFalls_1F_1R_Movement_AquaGrunt2GotoScene {
	walk_right * 8
	walk_up * 2
	walk_in_place_right
}

